    nome: caractere
    sobrenome: caractere

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
   ESCREVAL("Digite seu nome: ")
   LEIA(nome)
   ESCREVAL("Digite seu sobre nome: ")
   LEIA(sobrenome)
   ESCREVAL("---------------------LOADING------------------------- ")
   ESCREVAL("Seu nome é ", nome, " seu sobrenome é ",sobrenome)
   ESCREVAL("Então seu nome completo é ",nome, " ",sobrenome)



